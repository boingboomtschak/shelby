{
	"manifest_version": 2,
	"name": "Shelby Chrome Extension",
	"description": "An extension for Shelby",
	"version": "1.0",

  "background": {
    "scripts": ["background.js"//,"jquery-2.1.3.min.js"
    ]
  },

	"permissions": [
		   "webRequest","<all_urls>","storage"],
	"icons": { 
		"16": "img/logo.png",
		"48": "img/logo.png",
		"128": "img/logo.png" 
	},

	"browser_action": {
		"default_icon": {                    
			"19": "img/logo.png",          
			"38": "img/logo.png"          
		},
		"default_popup": "popup.html"
	},
	"content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
	"web_accessible_resources": [
		"img/icon128.png"
	]
}